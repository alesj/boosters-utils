node {
   def mvnHome
   stage('Preparation') {
    // Get some code from a GitHub repository
    git "https://github.com/alesj/boosters-tag"
      // Get the Maven tool.
      // ** NOTE: This 'M3' Maven tool must be configured
      // **       in the global configuration.
      mvnHome = tool 'M3'
   }
   stage('Build') {
    // Run the maven build
    sh "'${mvnHome}/bin/mvn' clean package"
   }
   stage('Deploy') {
    sh "java -jar ${WORKSPACE}/target/boosters-tag-1.0.0-SNAPSHOT.jar -b=upstream -lp=${WORKSPACE} -o=alesj -t=${params.token} -r=btagtest\$5 -q=https://api.github.com/users/%s/repos"
   }
}
